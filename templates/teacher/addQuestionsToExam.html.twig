{% extends 'baseLoggedIn.html.twig' %}

{% block content %}

    <div class="caption">
        Add Questions To Your Exam
    </div>

    <div class="actionFieldTopText">
        Please, complete the form below.
    </div>

    <div class="editQuestionForm">
        {{ form_start(form) }}
        {{ form_row(form.questions) }}
        {{ form_row(form.random) }}
        {{ form_row(form.save) }}
        {{ form_end(form) }}
    </div>
{% endblock %}